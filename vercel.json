{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1",
            "headers": {
                "Content-Type": "text/css"
            }
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1",
            "headers": {
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/",
            "dest": "/app.js"
        },
        {
            "src": "/class",
            "dest": "/app.js"
        },
        {
            "src": "/office",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
} 