{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/app.js"
        },
        {
            "src": "/class",
            "dest": "/app.js"
        },
        {
            "src": "/office",
            "dest": "/app.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
} 